# AWS Configuration for Development
# Copy this file to .env.local and fill in your values

# AWS Region
NEXT_PUBLIC_AWS_REGION=us-east-1

# DynamoDB Table Name (from CDK deployment)
NEXT_PUBLIC_DYNAMODB_TABLE_NAME=cost-optimization-scheduler-table

# For development only - NOT recommended for production
# In production, use proper authentication like AWS Cognito
NEXT_PUBLIC_AWS_ACCESS_KEY_ID=your_access_key_id_here
NEXT_PUBLIC_AWS_SECRET_ACCESS_KEY=your_secret_access_key_here
NEXT_PUBLIC_AWS_SESSION_TOKEN=your_session_token_here

# Alternative: Use AWS Profile (for server-side only)
# AWS_PROFILE=your-aws-profile-name
